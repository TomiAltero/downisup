{
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/lib/types.d.ts", "src/layouts/AuthLayout.", "src/app/application/panel-medico/[id]/page.jsx", "src/lib/utils.tss", "src/components/Calendar.jsx", "src/components/ChatBotComponent.jsx", "src/components/HorariosComponent.jsx", "src/components/TurnosGuardadosComponent.jsx", "src/components/Sidebar/index.jsx"],
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "exclude": ["node_modules"]
}
